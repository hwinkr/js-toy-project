<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Gym List</title>
</head>
<body>
    <h1>Gym List</h1>
    <a href="/gymlist/new">
        <button>Create New Gym!</button>
    </a>
    <ul>
        <% gyms.forEach((gym) => { %> 
            <a href="/gymlist/<%= gym._id %>">
                <li>Gym Name : <%= gym.title %> </li>
            </a>
        <% }) %> 
    </ul>
</body>
</html>